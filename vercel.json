{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.go",
      "use": "@vercel/go"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.go"
    },
    {
      "src": "/api/healthcheck",
      "dest": "/api/healthcheck.go"
    },
    {
      "src": "/api/py/(.*)",
      "dest": "/api/index.go"
    },
    {
      "src": "/generateERC7730",
      "dest": "/api/index.go"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "PYTHONUNBUFFERED": "1"
  }
} 